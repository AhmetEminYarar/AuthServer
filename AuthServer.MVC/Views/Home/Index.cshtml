@{
    ViewData["Title"] = "Home Page";
}
@using AuthServer.Data.Entity;
@model List<Person>

<div class="text-center">
    <table>
        <thead>
            <tr>
                <td>PersonImageURL</td>
                <td>Name</td>
                <td>Surname</td>
                <td>Age</td>
                <td>Password</td>
            </tr>
        </thead>
       <tbody>
           @foreach (var item in Model)
            {
               <tr>
                   <td>@item.PersonImageURL</td>
                   <td>@item.Name</td>
                   <td>@item.Surname</td>
                   <td>@item.Age</td>
                   <td>@item.Password</td>
               </tr>
            }
       </tbody>
        <tfoot>
            <tr>
                <td>PersonImageURL</td>
                <td>Name</td>
                <td>Surname</td>
                <td>Age</td>
                <td>Password</td>
            </tr>
        </tfoot>
    </table>
</div>
